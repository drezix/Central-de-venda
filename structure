    ├── controllers
    │   ├── SaleController.java
    │   └── SellerController.java
    ├── models
    │   ├── Sale.java
    │   └── Seller.java
    ├── repository
    │   ├── SaleRepository.java
    │   └── SellerRepository.java
    ├── services
    │   ├── SaleService.java
    │   └── SellerService.java
    └── SaleProjectApplication.java